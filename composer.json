{
    "name": "wisembly/totem",
    "license": "MIT",
    "description": "Changeset calculator between data states",
    "keywords": ["changeset"],

    "authors": [
        {
            "name": "Baptiste Clavi√©",
            "email": "clavie.b@gmail.com",
            "homepage": "http://baptiste.xn--clavi-fsa.net/",
            "role": "Lead Developper"
        }
    ],

    "support": {
        "issues": "https://github.com/Wisembly/Totem/issues",
        "source": "https://github.com/Wisembly/Totem"
    },

    "autoload": {
        "psr-4": {
            "Totem\\": ["src/"]
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Totem\\": ["test/"]
        }
    },

    "require": {
        "php": "^7.1",
        "php-ds/php-ds": "^1.0"
    },

    "require-dev": {
        "symfony/property-access": "^3.0"
    },

    "suggest": {
        "symfony/property-access": "Allows to use the collection snapshotter"
    },

    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev",
            "dev-1.x": "1.5-dev"
        }
    }
}

